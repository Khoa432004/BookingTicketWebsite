{
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/next",
        "config": {
          "buildCommand": "cd frontend && npm run vercel-build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "frontend/$1"
      }
    ]
  }